{
  "name": "eviction-lab-exports",
  "version": "0.0.2",
  "description": "Generate exports for Eviction Lab in Serverless Lambda function",
  "main": "src/exports/xlsx.fileHandler",
  "scripts": {
    "deploy-dev": "cp -r ./assets ./src/assets && sls deploy --stage dev && cd src && sls deploy --stage dev",
    "deploy": "cp -r ./assets ./src/assets && sls deploy --stage prod && cd src && sls deploy --stage prod",
    "offline-zip": "cp -r ./assets ./src/assets && sls offline",
    "offline-pdf": "cp -r ./assets ./src/assets && cd src && sls offline --port 3001",
    "postinstall": "cd ./node_modules/pptxgenjs && npm install && cd ../.."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/EvictionLab/eviction-lab-exports.git"
  },
  "author": "Eviction Lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvictionLab/eviction-lab-exports/issues"
  },
  "homepage": "https://github.com/EvictionLab/eviction-lab-exports#readme",
  "devDependencies": {
    "@types/node": "^13.1.7",
    "serverless": "^1.61.1",
    "serverless-domain-manager": "^3.3.0",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-typescript": "^1.1.9",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@mapbox/geo-viewport": "^0.4.0",
    "@types/handlebars": "^4.1.0",
    "aws-sdk": "^2.604.0",
    "axios": "^0.19.1",
    "canvas": "^2.6.1",
    "canvas-aws-prebuilt": "^2.0.2",
    "d3-scale": "^3.2.1",
    "d3-shape": "^1.3.7",
    "handlebars": "^4.7.2",
    "jszip": "^3.2.2",
    "pptxgenjs": "^3.0.1",
    "xlsx": "^0.15.4"
  }
}
